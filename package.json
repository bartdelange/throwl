    "prettier": "3.6.2",
    "react-test-renderer": "19.1.0",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "release-it": {
    "git": {
      "push": true,
      "tag": true,
      "requireBranch": "master"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": "npm run setVersion"
    }
  },
  "engines": {
    "node": ">=20"
  }
