{
  "name": "throwl",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android --terminal=iTerm",
    "prestart": "unset PREFIX; node scripts/write-xcode-env.js",
    "ios": "react-native run-ios --terminal=iTerm",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "setAndroidVersion": "cd android; bundle exec fastlane update_version version:$npm_package_version",
    "setIosVersion": "cd ios; bundle exec fastlane update_version version:$npm_package_version",
    "setVersion": "npm run setAndroidVersion && npm run setIosVersion",
    "release": "release-it",
    "postinstall": "patch-package",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "lint:check": "eslint ./src --max-warnings 0",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "format": "prettier --write . && eslint . --fix",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@freakycoder/react-native-helpers": "2.5.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/app": "^23.7.0",
    "@react-native-firebase/auth": "^23.7.0",
    "@react-native-firebase/firestore": "^23.7.0",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/material-top-tabs": "^7.4.11",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "@react-navigation/stack": "^7.6.13",
    "@sayem314/react-native-keep-awake": "^1.4.0",
    "ansi-regex": ">=6.2.2",
    "color-interpolate": "^2.0.0",
    "date-fns": "^4.1.0",
    "nanoid": ">=5.1.6",
    "node-fetch": ">=3.3.2",
    "nth-check": ">=2.1.1",
    "react": "19.2.3",
    "react-native": "0.83.1",
    "react-native-bootsplash": "^6.3.11",
    "react-native-bottom-search-bar": "^1.1.1",
    "react-native-bouncy-checkbox": "^4.1.2",
    "react-native-confetti": "^0.1.0",
    "react-native-dynamic-vector-icons": "^2.0.0",
    "react-native-easy-grid": "^0.2.2",
    "react-native-gesture-handler": "^2.30.0",
    "react-native-gifted-charts": "^1.4.70",
    "react-native-interactable": "^2.0.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-pager-view": "^8.0.0",
    "react-native-paper": "^5.14.5",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "^4.2.1",
    "react-native-responsive-linechart": "https://github.com/bartdelange/react-native-responsive-linechart.git",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-svg": "^15.15.1",
    "react-native-tab-view": "^4.2.2",
    "react-native-tts": "^4.1.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-worklets": "^0.7.1",
    "validator": "^13.15.26"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@react-native-community/cli": "^20.0.2",
    "@react-native-community/cli-platform-android": "^20.0.2",
    "@react-native-community/cli-platform-ios": "^20.0.2",
    "@react-native/babel-preset": "^0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "^0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@tsconfig/react-native": "^3.0.8",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.7",
    "@types/react-native-confetti": "^0.1.4",
    "@types/react-native-keep-awake": "^2.0.8",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "@types/validator": "^13.15.10",
    "babel-jest": "^30.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "patch-package": "^8.0.1",
    "pod-install": "^1.0.9",
    "prettier": "3.7.4",
    "react-test-renderer": "19.2.3",
    "release-it": "^19.2.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "release-it": {
    "git": {
      "push": true,
      "tag": true,
      "requireBranch": "master"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": "npm run setVersion"
    }
  },
  "engines": {
    "node": ">=20"
  }
}
