  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/runtime": "^7.17.9",
    "@types/jest": "^27.4.1",
    "@types/react-native": "^0.67.4",
    "@types/react-native-confetti": "^0.1.0",
    "@types/react-native-keep-awake": "^2.0.3",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
