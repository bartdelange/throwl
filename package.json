    "react-test-renderer": "17.0.2",
    "release-it": "^14.14.1",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "release-it": {
    "git": {
      "push": true,
      "tag": true,
      "requireBranch": "master"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "after:bump": "npm run setVersion"
    }
  }
}
